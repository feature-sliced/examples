{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "baseUrl": "./src",
    "module": "ESNext",
    "moduleResolution": "node",
    "paths": {
      "@/*": ["*"]
    },
    "typeRoots": ["node_modules/@types", "types/"],
    "types": ["vite/client"],
    "strict": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true
  }
}
